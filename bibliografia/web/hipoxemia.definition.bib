@article{CamiloJunior2009,
  author    = {Celso Gonçalves Camilo Junior and Reinaldo Gonçalves Nogueira and Cássio Dener Noronha Vinhal},
  title     = {Inteligência Artificial Distribuída: conhecendo para aplicar},
  journal   = {Revista Estudos - Revista de Ciências Ambientais e Saúde (EVS)},
  volume    = {35},
  number    = {2},
  pages     = {247--256},
  year      = {2009},
  month     = mar,
  doi       = {10.18224/est.v35i2.644},
  url       = {https://seer.pucgoias.edu.br/index.php/estudos/article/view/644},
  abstract  = {Inteligência artificial é uma área que chama atenção pela ousadia de implementar inteligência em seres não biológicos. No entanto, ao longo da história, criaram-se mitos causados por exageros a respeito de suas funcionalidades e aplicações, que, por consequência, geraram desconfianças. Com o intuito de levantar informações sobre a ciência Inteligência Artificial e sua disciplina Inteligência Artificial Distribuída, este artigo foi concebido. Algumas características, como o histórico, a disciplina Inteligência Artificial Distribuída e sua divisão, a infraestrutura que envolve aplicações nesta área e, por final, o estado da arte, serão apresentadas.},
  address   = {Goiânia, Brasil}
}
